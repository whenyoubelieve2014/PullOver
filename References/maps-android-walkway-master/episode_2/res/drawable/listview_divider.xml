<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#000" />
            <size
                android:width="1px"
                android:height="1px" />
        </shape>
    </item>
    
    <item android:top="1px">
        <shape android:shape="rectangle">
            <solid android:color="#666" />
            <size
                android:width="1px"
                android:height="1px" />
        </shape>
    </item>
</layer-list>
